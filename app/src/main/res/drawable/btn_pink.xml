<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="#009999" />
            <stroke
                android:width="1dp"
                android:color="#003333" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#FF66FF"
                android:endColor="#6F086F"
                android:angle="-90" />
            <stroke
                android:width="1dp"
                android:color="#350635" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
</selector>